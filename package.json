{
  "name": "ai-call-assistant",
  "version": "0.1.0",
  "description": "AI Call Assistant for real-time conversation assistance",
  "main": "dist/main/main.js",
  "author": "AICA Team",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"yarn dev:vite\" \"yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "cross-env NODE_ENV=development tsc -p tsconfig.electron.json && electron .",
    "build": "tsc -p tsconfig.electron.json && vite build --config vite.config.ts",
    "package": "electron-builder",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "langchain": "^0.1.0",
    "openai": "^4.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "eslint": "8",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "typescript": "^5.0.2",
    "typescript-eslint": "^8.25.0",
    "vite": "^4.4.5"
  },
  "packageManager": "yarn@4.6.0",
  "build": {
    "appId": "com.aica.app",
    "productName": "AI Call Assistant",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    }
  }
}
